{
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "linterOptions": {
        "exclude": [
            "test/**/*data.ts",
            "node_modules/**/*.ts"
        ]
    },
    "rules": {
        "interface-name": [
            true,
            "never-prefix"
        ]
    }
}